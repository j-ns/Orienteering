<?xml version="1.0" encoding="UTF-8" ?>
<configuration debug="false">

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <!-- encoders are assigned by default the type
         ch.qos.logback.classic.encoder.PatternLayoutEncoder -->
		<encoder>
			<pattern>%d{mm:ss.SSS} %-5level %logger{25}.%M: - %msg%n</pattern>
		</encoder>
	</appender>

	<logger name="com.j256.ormlite" level="OFF" />

<!-- 	<logger name="com.jns.orienteering.control.cell" level="OFF" /> -->
	<logger name="com.jns.orienteering.control.skin" level="OFF" />

	<root level="INFO">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>

<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import com.gluonhq.charm.glisten.control.*?>
<?import com.gluonhq.charm.glisten.mvc.*?>
<?import com.jfoenix.controls.JFXTabPane?>
<?import javafx.scene.control.Tab?>
<?import com.jns.orienteering.model.common.ListViewExtended?>

<View fx:id="view" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.jns.orienteering.view.ReportPresenter">
	<center>
		<JFXTabPane fx:id="tabPane">
			<tabs>
				<Tab fx:id="tabMissionStats" text="%tab.missions">
					<ListViewExtended fx:id="lviewMissionStats">
					</ListViewExtended>
				</Tab>
				<Tab fx:id="tabMissionStat" text="%tab.stat">
					<ListViewExtended fx:id="lviewMissionStat">
					</ListViewExtended>
				</Tab>
				<Tab fx:id="tabRanking" text="%tab.ranking">
					<ListViewExtended fx:id="lviewRanking">
					</ListViewExtended>
				</Tab>
			</tabs>
		</JFXTabPane>
	</center>
</View>
